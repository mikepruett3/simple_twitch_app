{
  "name": "simple_twitch_app",
  "version": "1.1.4",
  "productName": "Twitch App",
  "description": "A Twitch app with built in adblock",
  "main": "main.js",
  "scripts": {
    "test": "electron --trace-warnings .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package-mac": "electron-packager . twitch_app --overwrite --platform=darwin --arch=x64 --icon=twitch.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"simple twitch app\"",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikepruett3/simple_twitch_app.git"
  },
  "keywords": [
    "Electron",
    "Twitch"
  ],
  "author": {
    "name": "Mike Pruett",
    "email": "mikepruett3@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mikepruett3/simple_twitch_app/issues"
  },
  "homepage": "https://github.com/mikepruett3/simple_twitch_app#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7",
    "@electron-forge/maker-deb": "^7",
    "@electron-forge/maker-rpm": "^7",
    "@electron-forge/maker-squirrel": "^7",
    "@electron-forge/maker-zip": "^7",
    "@electron-forge/plugin-auto-unpack-natives": "^7",
    "@electron-forge/publisher-github": "^7",
    "electron": "^33.0.2",
    "electron-forge-maker-appimage": "github:Marcus10110/electron-forge-maker-appimage"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0"
  }
}
